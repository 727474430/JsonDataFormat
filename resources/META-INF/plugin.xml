<idea-plugin>
    <id>com.raindrop</id>
    <name>Json Data Format</name>
    <version>1.0 Release</version>
    <vendor email="qq727474430@gmail.com">Raindrop</vendor>

    <description><![CDATA[
        <html>Simple format the Json data into a beautiful format <br>
        <em>Default shortcuts: Alt + J + F</em> <br>
        <a href="https://github.com/727474430/JsonDataFormat">The source address</a>
        </html>
    ]]></description>

    <change-notes><![CDATA[
        <ul>
            <li>v1.0 Release</li>
        </ul>
    ]]>
    </change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="173.0"/>

    <actions>
        <!-- Add your actions here -->
        <action id="JsonDataFormat" class="com.raindrop.JsonDataFormat" text="Json Data Format"
                description="JsonDataFormat">
            <add-to-group group-id="CodeMenu" anchor="last"/>
            <keyboard-shortcut keymap="$default" first-keystroke="alt J" second-keystroke="F"/>
        </action>
    </actions>
    <extensions defaultExtensionNs="com.intellij"/>

</idea-plugin>